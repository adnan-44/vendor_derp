<fonts-modification version="1">
    <family customizationType="new-named-family" name="google-sans">
        <font weight="400" style="normal">GoogleSans-Regular.ttf</font>
        <font weight="400" style="italic">GoogleSans-Italic.ttf</font>
        <font weight="500" style="normal">GoogleSans-Medium.ttf</font>
        <font weight="500" style="italic">GoogleSans-MediumItalic.ttf</font>
        <font weight="700" style="normal">GoogleSans-Bold.ttf</font>
        <font weight="700" style="italic">GoogleSans-BoldItalic.ttf</font>
    </family>
    <alias name="google-sans-medium" to="google-sans" weight="500" />

    <family customizationType="new-named-family" name="inter">
        <font weight="100" style="normal">Inter-Thin.otf</font>
        <font weight="100" style="italic">Inter-ThinItalic.otf</font>
        <font weight="200" style="normal">Inter-ExtraLight.otf</font>
        <font weight="200" style="italic">Inter-ExtraLightItalic.otf</font>
        <font weight="300" style="normal">Inter-Light.otf</font>
        <font weight="300" style="italic">Inter-LightItalic.otf</font>
        <font weight="400" style="normal">Inter-Regular.otf</font>
        <font weight="400" style="italic">Inter-Italic.otf</font>
        <font weight="500" style="normal">Inter-Medium.otf</font>
        <font weight="500" style="italic">Inter-MediumItalic.otf</font>
        <font weight="700" style="normal">Inter-Bold.otf</font>
        <font weight="700" style="italic">Inter-BoldItalic.otf</font>
        <font weight="900" style="normal">Inter-Black.otf</font>
        <font weight="900" style="italic">Inter-BlackItalic.otf</font>
    </family>
    <alias name="inter-medium" to="inter" weight="500" />

    <family customizationType="new-named-family" name="arbutus-slab">
      <font weight="400" style="normal">ArbutusSlab-Regular.ttf</font>
    </family>

    <family customizationType="new-named-family" name="lato">
      <font weight="400" style="normal">Lato-Regular.ttf</font>
      <font weight="400" style="italic">Lato-Italic.ttf</font>
      <font weight="700" style="normal">Lato-Bold.ttf</font>
      <font weight="700" style="italic">Lato-BoldItalic.ttf</font>
    </family>
    <alias name="lato-bold" to="lato" weight="700" />

    <family customizationType="new-named-family" name="rubik">
      <font weight="400" style="normal">Rubik-Regular.ttf</font>
      <font weight="400" style="italic">Rubik-Italic.ttf</font>
      <font weight="500" style="normal">Rubik-Medium.ttf</font>
      <font weight="500" style="italic">Rubik-MediumItalic.ttf</font>
      <font weight="700" style="normal">Rubik-Bold.ttf</font>
      <font weight="700" style="italic">Rubik-BoldItalic.ttf</font>
    </family>
    <alias name="rubik-medium" to="rubik" weight="500" />

    <family customizationType="new-named-family" name="zilla-slab-medium">
      <font weight="500" style="normal">ZillaSlab-Medium.ttf</font>
      <font weight="500" style="italic">ZillaSlab-MediumItalic.ttf</font>
      <font weight="600" style="normal">ZillaSlab-SemiBold.ttf</font>
      <font weight="600" style="italic">ZillaSlab-SemiBoldItalic.ttf</font>
    </family>
    <alias name="zilla-slab-semi-bold" to="zilla-slab-medium" weight="600" />

    <family customizationType="new-named-family" name="karla">
      <font weight="400" style="normal">Karla-Regular.ttf</font>
    </family>
    <alias name="karla" to="karla" weight="400"/>

    <family customizationType="new-named-family" name="fraunces">
      <font weight="400" style="normal">Fraunces-Regular.ttf</font>
      <font weight="600" style="normal">Fraunces-SemiBold.ttf</font>
    </family>
    <alias name="fraunces" to="fraunces" weight="400"/>
    <alias name="fraunces-semi-bold" to="fraunces" weight="600"/>

    <family customizationType="new-named-family" name="big-shoulders-text">
      <font weight="700" style="normal">BigShouldersText-Bold.ttf</font>
      <font weight="800" style="normal">BigShouldersText-ExtraBold.ttf</font>
    </family>
    <alias name="big-shoulders-text-bold" to="big-shoulders-text" weight="700"/>
    <alias name="big-shoulders-text-extra-bold" to="big-shoulders-text" weight="800"/>

    <family customizationType="new-named-family" name="barlow">
      <font weight="700" style="normal">Barlow-Bold.ttf</font>
      <font weight="500" style="normal">Barlow-Medium.ttf</font>
    </family>
    <alias name="barlow-bold" to="barlow" weight="700"/>
    <alias name="barlow-medium" to="barlow" weight="500"/>

    <family customizationType="new-named-family" name="Slate">
        <font weight="300" style="normal">SlateForOnePlus-Book.ttf</font>
        <font weight="300" style="italic">Roboto-LightItalic.ttf</font>
        <font weight="400" style="normal">SlateForOnePlus-Regular.ttf</font>
        <font weight="400" style="italic">Roboto-Italic.ttf</font>
        <font weight="500" style="normal">SlateForOnePlus-Medium.ttf</font>
        <font weight="500" style="italic">Roboto-MediumItalic.ttf</font>
    </family>
    <alias name="Slate" to="Slate" weight="400"/>
    <alias name="Slate-book" to="Slate" weight="300" />
    <alias name="Slate-medium" to="Slate" weight="500" />

    <family customizationType="new-named-family" name="ComicSans">
        <font weight="400" style="normal">comic.ttf</font>
        <font weight="400" style="italic">comici.ttf</font>
        <font weight="700" style="normal">comicbd.ttf</font>
        <font weight="700" style="italic">comicz.ttf</font>
    </family>
    <alias name="ComicSans-Bold" to="ComicSans" weight="700" />

    <!-- Btlg fontage -->
    <family customizationType="new-named-family" name="adamcg-pro">
        <font weight="300" style="normal">AdamCGPro-Regular.ttf</font>
    </family>
    <family customizationType="new-named-family" name="bignoodle-italic">
        <font weight="300" style="normal">BigNoodleTilting-Italic.ttf</font>
    </family>
    <family customizationType="new-named-family" name="bignoodle-regular">
        <font weight="300" style="normal">BigNoodleTilting-Regular.ttf</font>
    </family>
    <family customizationType="new-named-family" name="biko">
        <font weight="300" style="normal">Biko-Regular.ttf</font>
    </family>
    <family customizationType="new-named-family" name="exo2-regular">
        <font weight="300" style="normal">Exo2-Regular.ttf</font>
    </family>
    <family customizationType="new-named-family" name="exo2-semibold">
        <font weight="300" style="normal">Exo2-SemiBold.ttf</font>
    </family>
    <family customizationType="new-named-family" name="ginora-sans">
        <font weight="300" style="normal">GinoraSans-Regular.ttf</font>
    </family>
    <family customizationType="new-named-family" name="googlesans-sys">
        <font weight="300" style="normal">GoogleSans-Medium.ttf</font>
    </family>
    <family customizationType="new-named-family" name="hanken">
        <font weight="300" style="normal">Hanken-Light.ttf</font>
    </family>
    <family customizationType="new-named-family" name="mittelschrift">
        <font weight="300" style="normal">din1451alt.ttf</font>
    </family>
    <family customizationType="new-named-family" name="reemkufi">
        <font weight="300" style="normal">ReemKufi-Regular.ttf</font>
    </family>
    <family customizationType="new-named-family" name="tinkerbell">
        <font weight="300" style="normal">GearTinkerbell.ttf</font>
    </family>
</fonts-modification>
